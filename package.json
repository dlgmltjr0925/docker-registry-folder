    "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
    "test": "jest --config ./test/jest.json",
    "test:cov": "echo $(pwd)/test/coverage/lcov-report/index.html && yarn test --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --coverage"
