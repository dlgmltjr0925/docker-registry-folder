{
  "moduleFileExtensions": ["js", "json", "ts"],
  "modulePathIgnorePatterns": ["<rootDir>/main.ts", "<rootDir>/app.module.ts"],
  "rootDir": "../src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../test/coverage",
  "testEnvironment": "node"
}
